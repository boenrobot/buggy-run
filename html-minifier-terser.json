{
    "caseSensitive": false,
    "collapseBooleanAttributes": true,
    "collapseWhitespace": true,
    "conservativeCollapse": false,
    "collapseInlineTagWhitespace": false,
    "preserveLineBreaks": false,
    "noNewlinesBeforeTagClose": true,
    "decodeEntities": true,
    "html5": true,
    "useShortDoctype": true,
    "minifyCSS": true,
    "minifyJS": true,
    "removeComments": true,
    "sortAttributes": true,
    "sortClassName": true
}